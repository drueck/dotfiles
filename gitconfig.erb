[user]
	name = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %>
	email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[alias]
	graph = log --graph --oneline --all --decorate
	co = checkout
[color]
	ui = true
[core]
	editor = vi
	pager = less -x1,3
	autocrlf = input
[push]
	default = simple
